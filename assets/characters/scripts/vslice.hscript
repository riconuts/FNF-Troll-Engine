// vslice cumpat

function setupCharacter(){
	super();
	if(game == PlayState.instance && game.stage.stageData.alternate_char_pos != true){
		this.x -= this.width / 2;
		this.y -= this.height;
	}
}

function getCamera(){
    var charCenterX = this.x + this.width / 2;
    var charCenterY = this.y + this.height / 2;
	return [
		charCenterX + this.cameraPosition[0],
		charCenterY + this.cameraPosition[1]
	];
}
	